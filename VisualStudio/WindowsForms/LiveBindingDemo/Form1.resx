  <data name="textBox25.Text" xml:space="preserve">
    <value>Values can be converted in both directions by Converter objects. For example, StatusToColorConverter provides a color value based on operation status (here: yellow background when quality is Uncertain).</value>
  </data>
  <metadata name="statusToColorConverter1.TrayLocation" type="System.Drawing.Point, System.Drawing, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a">
